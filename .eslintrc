{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended",
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
  },
  "rules": {
    "no-console": "warn",
    "@typescript-eslint/explicit-module-boundary-types": "on",
  }
}
